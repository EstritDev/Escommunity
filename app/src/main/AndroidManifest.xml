<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MyUniverse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MyUniverse"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Escommunity">
        <activity
            android:name=".activities.ProcurarActivity"
            android:exported="true" />
        <activity
            android:name=".activities.EditPostActivity"
            android:exported="true" />
        <activity
            android:name=".activities.EditarPerfilActivity"
            android:exported="true" />
        <activity
            android:name=".activities.NovoPostActivity"
            android:exported="true" />
        <activity
            android:name=".activities.PerfilActivity"
            android:exported="true" />
        <activity
            android:name=".activities.RegistarActivity"
            android:exported="true" />
        <activity
            android:name=".activities.PaginaInicial"
            android:exported="true" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>